{
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:5175",
            "webRoot": "${workspaceFolder}",
            "sourceMapPathOverrides": {
                "webpack:///./*": "${webRoot}/*"
            },
            "resolveSourceMapLocations": [
                "${workspaceFolder}/",
                "!/node_modules/**"
            ],
        }
    ]
}